<odoo>
  <record id="model_access_todo" model="ir.model.access">
    <field name="name">model.access.todo</field>
    <field name="model_id" ref="todo_app.model_todo_app"/>
    <field name="group_id" ref="base.group_system"/>
    <!-- aqui es verdadero o falso y tambien puede ser con 1 o 0 -->
    <!-- Aqui se encuentran los permisos editar,eliminar,etc -->
    <field name="perm_read" eval="1"/>
    <field name="perm_create" eval="1"/>
    <field name="perm_write" eval="1"/>
    <field name="perm_unlink" eval="1"/>
  </record>
</odoo>